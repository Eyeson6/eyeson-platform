<template menuId="201" version="2"><!--模板跟节点  -->

    <!-- 在页面上显示页面的所在位置 -->
    <directions>
        <direct name="采购管理"/>
        <direct name="首营企业业务审核"/>
    </directions>

    <!-- 将模块所有sql语句统一配置，供各个操作使用，其中包含多个sql配置-->
    <sqls sql="true">
        <sql id="listsql" method="select">
            SELECT
            id,

            CASE category_id
            WHEN '0' THEN
            '经营单位'
            WHEN '1' THEN
            '生产单位'
            ELSE '其他' END
            as category_name,

            CASE settlement_id
            WHEN '0' THEN
            '现金'
            WHEN '1' THEN
            '月票'
            WHEN '2' THEN
            '支票'
            ELSE '其他' END
            as settlement_name,

            area_code,
            pinyin_code,
            name,
            code,
            address,
            contact,
            tel,
            bank_code,
            bank_name,
            bank_account,
            business_license_code,
            business_license_date,
            business_license_expire_date,
            business_license_approval,
            license_code,
            license_date,
            license_expire_date,
            license_name,
            consigner,
            consign_date,
            consign_expire_date,
            quality_license_expire_date,
            health_license_expire_date,
            gsp_code,
            gsp_date,
            gsp_expire_date,
            gmp_one_code,
            gmp_one_date,
            gmp_one_expire_date,
            gmp_two_code,
            gmp_two_date,
            gmp_two_expire_date,
            business_scope,
            quality_head,
            certification_scope,
            tax_registration_code,
            registered_capital,
            business_pattern,
            economic_nature,
            legal_person,

            CASE has_annual_inspection
            WHEN '0' THEN
            '否'
            WHEN '1' THEN
            '是'
            ELSE '其他' END
            as has_annual_inspection,

            CASE has_seal
            WHEN '0' THEN
            '否'
            WHEN '1' THEN
            '是'
            ELSE '其他' END
            as has_seal,

            CASE has_manifest
            WHEN '0' THEN
            '否'
            WHEN '1' THEN
            '是'
            ELSE '其他' END
            as has_manifest,

            oraganization_code_expire_date,
            annual_output_value,
            quality_manager_count,
            business_approval_user_id,
            business_approval_user_name,

            CASE business_approval_status
            WHEN '0' THEN
            '待审核'
            WHEN '1' THEN
            '驳回'
            WHEN '2' THEN
            '通过'
            ELSE '其他' END
            as business_approval_status,

            business_approval_time,
            quality_manager_approval_user_id,
            quality_manager_approval_user_name,

            CASE quality_manager_approval_status
            WHEN '0' THEN
            '待审核'
            WHEN '1' THEN
            '驳回'
            WHEN '2' THEN
            '通过'
            ELSE '其他' END
            as quality_manager_approval_status,

            quality_manager_approval_time,
            quality_approval_user_id,
            quality_approval_user_name,

            CASE quality_approval_status
            WHEN '0' THEN
            '待审核'
            WHEN '1' THEN
            '驳回'
            WHEN '2' THEN
            '通过'
            ELSE '其他' END
            as quality_approval_status,

            quality_approval_time,
            creater_name,
            create_date,
            update_date
            FROM
            saas_supplier where
            1=1 and delete_state = '0' and business_approval_status=0
            <if method="isNoEmpty" args="${startTime}">
                <![CDATA[and create_date >= ${startTime}]]>
            </if>
            <if method="isNoEmpty" args="${endTime}">
                <![CDATA[ and create_date <= ${endTime}]]>
            </if>
            <if method="isNoEmpty" args="${name}">
                <![CDATA[ and name like %${name}%]]>
            </if>
            <if method="isNoEmpty" args="${pinyin_code}">
                <![CDATA[ and pinyin_code like %${pinyin_code}%]]>
            </if>
            <if method="isNoEmpty" args="${creater_name}">
                <![CDATA[ and creater_name like %${creater_name}%]]>
            </if>
            order by business_approval_time desc
        </sql>
        <sql id="querySql" method="query">
            select * from saas_supplier where id=${key}
        </sql>


        <sql id="updateSql" method="update">
            update saas_supplier set business_approval_time=now(),
            <if method="isNoEmpty" args="${business_approval_user_id}">
                business_approval_user_id=${business_approval_user_id},
            </if>
            <if method="isNoEmpty" args="${business_approval_user_name}">
                business_approval_user_name=${business_approval_user_name},
            </if>
            <if method="isNoEmpty" args="${business_approval_status}">
                business_approval_status=${business_approval_status},
            </if>
            <if method="isNoEmpty" args="${business_approval_reason}">
                business_approval_reason=${business_approval_reason},
            </if>
            <if method="isNoEmpty" args="${quality_manager_approval_status}">
                quality_manager_approval_status=${quality_manager_approval_status},
            </if>
            where id=${key}
        </sql>

        <sql id="totalSql" method="select">
            select count(a.channels_id) channelsid
            from KKMY_CHANNELS a where 1=1
            <if method="isNoEmpty" args="${startTime}">
                <![CDATA[and a.create_time >= ${startTime}]]>
            </if>
            <if method="isNoEmpty" args="${endTime}">
                <![CDATA[ and a.create_time <= ${endTime}]]>
            </if>
            <if method="isNoEmpty" args="${channels_name}">
                <![CDATA[ and a.channels_name like %${channels_name}%]]>
            </if>
            <if method="isNoEmpty" args="${docking_people}">
                <![CDATA[ and a.channels_id =${docking_people}]]>
            </if>
        </sql>

    </sqls>

    <!-- 查询节点，配包括查询请求的执行流程、页面渲染方式、查询条件配置等几个部分 -->
    <query pagesize="50">
